{
  "name": "@container-server/container-server",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "clean": "yarn run rimraf node_modules yarn.lock build && yarn cache clean",
    "ci": "yarn install --frozen-lockfile",
    "ci:audit": "yarn run improved-yarn-audit --min-severity moderate",
    "ci:audit:preview": "yarn run yarn-audit-fix --audit-level moderate --dry-run",
    "ci:audit:fix": "yarn run yarn-audit-fix --audit-level moderate",
    "precommit": "yarn run pretty && yarn run lint && yarn run tsc && yarn run test",
    "pretty": "prettify",
    "lint": "eslint --ext .ts,.json .",
    "lint:fix": "eslint --ext .ts,.json --fix",
    "tsc": "tsc --noEmit",
    "tests": "yarn run jest --passWithNoTests",
    "coverage": "yarn run jest --collect-coverage",
    "bundle": "tsc -outDir build",
    "build": "f() { bash scripts/build.sh --service api --env $1; }; f",
    "push": "f() { bash scripts/push.sh --service api --env $1; }; f",
    "local": "DOTENV_CONFIG_PATH=.env.local ts-node src/index.ts",
    "start": "f() { DOTENV_CONFIG_PATH=.env.$1 node -r dotenv/config build/index.js; }; f"
  },
  "dependencies": {
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "helmet": "7.0.0",
    "hpp": "0.2.3",
    "openapi-types": "12.1.3",
    "winston": "3.8.2",
    "winston-daily-rotate-file": "4.7.1"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/hpp": "0.2.2",
    "@types/jest": "29.5.10",
    "@types/node": "20.5.3",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "esbuild": "0.17.18",
    "eslint": "8.54.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-json-format": "2.0.1",
    "prettier": "3.0.3",
    "jest": "29.7.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "yarn-audit-fix": "9.3.6"
  }
}